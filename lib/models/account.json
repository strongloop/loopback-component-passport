{
  "name": "Account",
  "plural": "Accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "emailVerificationRequired": true
  },
  "mixins": {
    "TimeStamp": {
      "validateUpsert": true,
      "silenceWarnings": true,
      "required": false
    }
  },
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "image": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "assets": {
      "type": "hasMany",
      "model": "Asset",
      "foreignKey": "ownerId"
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "foreignKey": "sourceAccountId"
    },
    "posts": {
      "type": "hasAndBelongsToMany",
      "model": "Post",
      "foreignKey": ""
    },
    "sites": {
      "type": "hasMany",
      "model": "Site",
      "foreignKey": "ownerId"
    },
    "roleMapping": {
      "type": "hasAndBelongsToMany",
      "model": "RoleMapping",
      "foreignKey": "ownerId"
    },
    "specifications": {
      "type": "hasMany",
      "model": "Specification",
      "foreignKey": "ownerId"
    },
    "specificationCapabilityItems": {
      "type": "hasMany",
      "model": "SpecificationCapabilityItem",
      "foreignKey": "ownerId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "CustomAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "socialAccounts": {
      "type": "hasMany",
      "model": "SocialAccount",
      "foreignKey": ""
		},
		"identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__sites"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__assets"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "sendEmail"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "botLogin"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
